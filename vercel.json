{
  "version": 2,
  "name": "dr-anja-haile-psychotherapie",
  "buildCommand": "next build",
  "outputDirectory": "out",
  "rewrites": [
    {
      "source": "/((?!_next|images|favicon\\.ico).*)",
      "destination": "/$1.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "trailingSlash": true
}